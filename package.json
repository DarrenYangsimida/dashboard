{
  "name": "kubegems-dashboard",
  "version": "1.22.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "vite --force --cors -l info",
    "build": "vite build && rm -rf dist/js/*.map dist/*.map",
    "preview": "vite preview -l info -d",
    "lint": "eslint ./ --ext .ts,.tsx,.vue,.js,.jsx",
    "prepare": "husky install",
    "cz": "git add . && git cz",
    "amend": "git add . && git commit --amend"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,vue}": [
      "eslint ./ --ext .ts,.tsx,.vue,.js,.jsx --fix",
      "git add"
    ],
    "**/*.{html,vue,css,sass,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "commit": true
    }
  },
  "dependencies": {
    "@iconify-json/logos": "^1.1.17",
    "@iconify-json/mdi": "^1.1.34",
    "@iconify/vue2": "^1.2.1",
    "ajv": "^8.6.3",
    "axios": "^0.27.2",
    "brace": "^0.11.1",
    "chart.js": "^3.9.1",
    "chartjs-adapter-moment": "^1.0.0",
    "chartjs-plugin-doughnutlabel-v3": "^1.2.0",
    "fast-json-patch": "^3.1.1",
    "filepond": "^4.30.4",
    "filepond-plugin-file-validate-size": "^2.2.7",
    "filepond-plugin-file-validate-type": "^1.2.8",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^11.5.1",
    "js-base64": "^3.6.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^12.0.6",
    "moment": "^2.29.4",
    "v-code-diff": "^0.3.12",
    "vue": "^2.7.13",
    "vue-clipboard2": "^0.3.3",
    "vue-filepond": "^7.0.3",
    "vue-form-wizard": "0.8.4",
    "vue-i18n": "^8.27.2",
    "vue-meta": "^2.4.0",
    "vue-okr-tree": "^1.0.12",
    "vue-router": "^3.4.4",
    "vue-slider-component": "^2.8.16",
    "vue-table-dynamic": "^0.4.10",
    "vue2-ace-editor": "^0.0.15",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.6.12",
    "vuex": "^3.6.2",
    "xterm": "^4.19.0",
    "xterm-addon-fit": "^0.6.0",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.0.0",
    "@vitejs/plugin-vue2-jsx": "^1.0.3",
    "@volar-plugins/vetur": "^0.1.0",
    "commitizen": "^4.2.4",
    "commitlint": "^13.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "eslint-plugin-vuetify": "^1.1.0",
    "happy-dom": "^7.4.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.6.2",
    "right-pad": "^1.0.1",
    "sass": "~1.26.5",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.8",
    "vite-plugin-chunk-split": "^0.4.1",
    "vite-plugin-md": "^0.20.4"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not ie <= 11",
    "not dead"
  ]
}
